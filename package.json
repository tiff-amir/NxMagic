{
  "name": "NxMagic",
  "version": "0.1.0",
  "scripts": {
    "release": "bash ./scripts/release.sh",
    "dev": "yarn addresses && yarn dev-light",
    "dev-light": "next -p 8081",
    "build": "yarn td && yarn build-light",
    "build-light": "yarn addresses && next build",
    "build-export": "yarn build-light && next export",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc",
    "svg": "node scripts/svg-sprite-generator/index.js",
    "addresses": "node scripts/update-addresses.js",
    "creat-root": "truffle-flattener solidity/contracts/NxMagic.sol > ./solidity/contracts/Root.sol",
    "ganache-local": "ganache-cli --defaultBalanceEther 1000 --gasLimit 0xfffffffffff --port 7545",
    "truffle-test": "truffle test --network local",
    "truffle-deploy": "truffle deploy --network local",
    "deploy": "node scripts/deploy.js"
  },
  "eslintConfig": {
    "globals": {
      "artifacts": "readonly",
      "contract": "readonly"
    }
  },
  "jest": {
    "verbose": true,
    "testTimeout": 1000000,
    "testMatch": [
      "**/_integration/**/*.[jt]s?(x)"
    ]
  },
